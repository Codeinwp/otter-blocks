{
    "name": "codeinwp/otter-blocks",
    "description": "A set of awesome Gutenberg Blocks by ThemeIsle.",
    "type": "wordpress-plugin",
    "version": "1.3.2",
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.3",
        "wp-coding-standards/wpcs": "^1",
        "phpcompatibility/php-compatibility": "^9",
        "dealerdirect/phpcodesniffer-composer-installer": "0.5.0",
        "automattic/vipwpcs": "^0.3.0"
    },
    "license": "GPL-2.0+",
    "authors": [
        {
            "name": "ThemeIsle Team",
            "email": "friends@themeisle.com"
        }
    ],
    "scripts": {
        "lint": "phpcs",
        "format": "phpcbf"
    },
    "prefer-stable": true,
    "config": {
        "optimize-autoloader": true
    },
    "extra": {
        "installer-disable": "true"
    },
    "minimum-stability": "dev",
    "require": {
        "codeinwp/themeisle-sdk": "^3.1",
        "codeinwp/gutenberg-blocks": "^1.3",
        "codeinwp/gutenberg-css": "^1.0",
        "codeinwp/gutenberg-animation": "^1.0"
    }
}
