{
	"name": "codeinwp/otter-blocks",
	"description": "A set of awesome Gutenberg Blocks by ThemeIsle.",
	"type": "wordpress-plugin",
	"version": "2.6.6",
	"require-dev": {
		"squizlabs/php_codesniffer": "^3.3",
		"wp-coding-standards/wpcs": "^1",
		"phpcompatibility/php-compatibility": "^9",
		"dealerdirect/phpcodesniffer-composer-installer": "0.7.1",
		"automattic/vipwpcs": "^1.0.0",
		"phpunit/phpunit": "9.6.5",
		"yoast/phpunit-polyfills": "^2.0",
		"phpstan/phpstan": "^1.10",
		"szepeviktor/phpstan-wordpress": "^1.3",
		"php-stubs/woocommerce-stubs": "^8.0",
		"php-stubs/acf-pro-stubs": "^6.0",
		"spaze/phpstan-stripe": "^2.4"
	},
	"license": "GPL-2.0+",
	"authors": [
		{
			"name": "ThemeIsle Team",
			"email": "friends@themeisle.com"
		}
	],
	"scripts": {
		"lint": "phpcs",
		"format": "phpcbf",
		"phpunit": "phpunit",
		"phpstan": "phpstan analyse --memory-limit 2G"
	},
	"prefer-stable": true,
	"config": {
		"optimize-autoloader": true,
		"platform": {
			"php": "7.4"
		},
		"allow-plugins": {
			"dealerdirect/phpcodesniffer-composer-installer": true
		}
	},
	"extra": {
		"installer-disable": "true"
	},
	"autoload": {
		"classmap": ["inc/"],
		"files": [
			"vendor/codeinwp/themeisle-sdk/load.php"
		]
	},
	"autoload-dev": {
		"classmap": ["plugins/otter-pro/inc/"],
		"files": [
			"development.php"
		]
	},
	"minimum-stability": "dev",
	"require": {
		"codeinwp/themeisle-sdk": "^3.2",
		"masterminds/html5": "^2.7",
		"tubalmartin/cssmin": "^4.1",
		"wptt/webfont-loader": "^1.1",
		"sabberworm/php-css-parser": "^8.4",
		"stripe/stripe-php": "^13.1",
		"enshrined/svg-sanitize": "^0.18.0"
	}
}
