{
	"name": "codeinwp/otter-blocks",
	"description": "A set of awesome Gutenberg Blocks by ThemeIsle.",
	"type": "wordpress-plugin",
	"version": "3.1.1",
    "repositories":[
        {
            "type":"composer",
            "url":"https://wpackagist.org",
            "only": [
                "wpackagist-plugin/*"
            ]
        }
    ],
	"require-dev": {
		"squizlabs/php_codesniffer": "^3.3",
		"wp-coding-standards/wpcs": "^3.2",
		"phpcompatibility/php-compatibility": "^9",
		"dealerdirect/phpcodesniffer-composer-installer": "0.7.1",
		"phpunit/phpunit": "9.6.5",
		"yoast/phpunit-polyfills": "^4.0",
		"phpstan/phpstan": "^2.1",
		"php-stubs/woocommerce-stubs": "^10.1",
		"php-stubs/acf-pro-stubs": "^6.0",
		"wpackagist-plugin/woocommerce": "*",
		"szepeviktor/phpstan-wordpress": "^2.0",
		"automattic/vipwpcs": "^3.0"
	},
	"license": "GPL-2.0+",
	"authors": [
		{
			"name": "ThemeIsle Team",
			"email": "friends@themeisle.com"
		}
	],
	"scripts": {
		"lint": "phpcs",
		"format": "phpcbf",
		"phpunit": "phpunit",
		"phpstan": "phpstan analyse --memory-limit 2G",
		"phpstan:generate:baseline": "phpstan analyse --memory-limit 2G --generate-baseline",
		"format-dist": "phpcbf --standard=phpcs.xml.dist"
	},
	"prefer-stable": true,
	"config": {
		"optimize-autoloader": true,
		"platform": {
			"php": "7.4"
		},
		"allow-plugins": {
			"dealerdirect/phpcodesniffer-composer-installer": true,
			"composer/installers": true
		}
	},
	"extra": {
		"installer-disable": "true",
		"installer-paths": {
			"vendor/wp-content/plugins/{$name}/": ["type:wordpress-plugin"]
		}
	},
	"autoload": {
		"classmap": ["inc/"],
		"files": [
			"vendor/codeinwp/themeisle-sdk/load.php"
		]
	},
	"autoload-dev": {
		"classmap": ["plugins/otter-pro/inc/"],
		"files": [
			"development.php"
		]
	},
	"minimum-stability": "dev",
	"require": {
		"codeinwp/themeisle-sdk": "^3.2",
		"masterminds/html5": "^2.7",
		"tubalmartin/cssmin": "^4.1",
		"wptt/webfont-loader": "^1.1",
		"sabberworm/php-css-parser": "^8.4",
		"stripe/stripe-php": "^15.3",
		"enshrined/svg-sanitize": "^0.21.0"
	}
}
